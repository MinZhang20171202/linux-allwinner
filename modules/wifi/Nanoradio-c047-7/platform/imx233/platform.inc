KERNEL_DIR=$(IMX233_KERNEL_DIR)
CROSS_COMPILE=$(IMX233_CROSS_COMPILE)
KERNEL_MAJOR_VERSION=2.6
export ARCH=arm

SDIO_HOST=imx31
TRANSPORT=ksdio
HOST=arm-none-linux-gnueabi

PLATFORM_FLAGS+=-DUSE_SDIO_4BIT_MODE
PLATFORM_FLAGS+=-DFORCE_SYSCTL_SYSCALL_CHECK

PLATFORM_FLAGS+=-DKSDIO_SIZE_ALIGN=8
PLATFORM_FLAGS+=-DKSDIO_NO_BLOCK_MODE
PLATFORM_FLAGS+=-DKSDIO_WAKEUP_CLOCK=80000
